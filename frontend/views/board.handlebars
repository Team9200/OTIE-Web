<div class="ui container">
    {{#each result}}
    <div class="ui fluid card">
        <div class="content">
            <div class="header">{{this.sha256}}</div>
            <div class="meta">Analyzer: {{this.analyzer}} / Collector: {{this.collector}}</div>
            <div class="description">
                <ul>
                    <li>File Type: {{this.filetype}}</li>
                    <li>First seen: {{this.first_seen}}</li> 
                    <li>Posted Date: {{this.date}}</li> 
                </ul>
            </div>
        </div>
        <div class="extra content">
            <i class="check icon"></i>
            121 Votes
        </div>
    </div>
    {{/each}}

</div>